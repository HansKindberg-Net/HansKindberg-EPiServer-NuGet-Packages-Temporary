<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<metadata>
		<dependencies xdt:Transform="Replace">
			<dependency id="$ApplicationFilesPackageId$" version="[$ApplicationFilesPackageVersion$]" />
		</dependencies>
		<description xdt:Transform="Replace">A package with build-targets for EPiServer $EPiServerVersion$, config-transform etc.</description>
		<references xdt:Transform="Remove" />
		<releaseNotes xdt:Transform="Remove" />
		<summary xdt:Transform="Remove" />
	</metadata>
	<files xdt:Transform="Replace">
		<file src="$TemporaryDirectory$$Id$.$Version$.props" target="build\$Id$.props" />
		<file src="$TemporaryDirectory$$Id$.targets" target="build" />
		<file src="$ProjectDirectory$ReadMe\Build.ReadMe.txt" target="ReadMe.txt" />
	</files>
</package>