<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003" xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<ItemGroup Condition="@(ConfigurationTemplate) == ''" xdt:Transform="Insert">
		<ConfigurationTemplate Include="$(ConfigurationPath)log4net.Template.config">
			<Destination>log4net.Template.config</Destination>
			<Transform>log4net.Common.config</Transform>
		</ConfigurationTemplate>
		<ConfigurationTemplate Include="$(ConfigurationPath)\Web.Template.config">
			<Destination>Web.Template.config</Destination>
			<Transform>Web.Common.config</Transform>
		</ConfigurationTemplate>
	</ItemGroup>
	<ItemGroup Condition="$(EPiServerBinariesToCopyToBin) == ''" xdt:Transform="Insert">
		<EPiServerBinariesToCopyToBin Include="${EPiServerBinariesToCopyToBin}" />
	</ItemGroup>
	<PropertyGroup xdt:Transform="Insert">
		<ApplicationFilesPackageVersion Condition="$(ApplicationFilesPackageVersion) == ''">${ApplicationFilesPackageVersion}</ApplicationFilesPackageVersion>
		<BinariesPackageVersion Condition="$(BinariesPackageVersion) == ''">${BinariesPackageVersion}</BinariesPackageVersion>
		<ConfigurationPath Condition="$(ConfigurationPath) == ''">$(NuGetPackageDirectory)${ApplicationFilesPackageName}.${ApplicationFilesPackageVersion}\Configuration\</ConfigurationPath>
		<MessageImportance Condition="$(MessageImportance) == ''">${MessageImportance}</MessageImportance>
		<NuGetPackageDirectory Condition="$(NuGetPackageDirectory) == ''">${NuGetPackageDirectory}</NuGetPackageDirectory>
	</PropertyGroup>
</Project>