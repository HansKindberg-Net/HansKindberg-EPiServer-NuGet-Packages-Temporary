<?xml version="1.0" encoding="utf-8"?>
<log4net xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<appender name="ConfigurationAppender" xdt:Locator="Match(name)">
		<layout type="log4net.Layout.PatternLayout">
			<conversionPattern value="Debug - [%thread] %level %logger: %message%n" xdt:Transform="SetAttributes" />
		</layout>
	</appender>
</log4net>