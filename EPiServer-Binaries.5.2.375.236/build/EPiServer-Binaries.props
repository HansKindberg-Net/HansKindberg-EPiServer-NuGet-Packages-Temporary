	<PropertyGroup>
		<EPiServerVersion Condition="$(EPiServerVersion) == ''">5.2.375.236</EPiServerVersion>
		<EPiServerBinariesMessageImportance Condition="$(EPiServerBinariesMessageImportance) == ''">high</EPiServerBinariesMessageImportance>
	</PropertyGroup>
