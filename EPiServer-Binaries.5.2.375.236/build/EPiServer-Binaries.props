<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ItemGroup Condition="$(EPiServerBinariesToCopyToBin) == ''">
		<EPiServerBinariesToCopyToBin Include="$(SolutionDir)packages\EPiServer-Binaries.$(EPiServerVersion)\lib\net20\**\*.dll" />
		<EPiServerBinariesToCopyToBin Include="$(SolutionDir)packages\log4net.1.2.10\lib\2.0\**\*.dll" />
