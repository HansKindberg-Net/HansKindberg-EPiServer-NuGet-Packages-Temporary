<?xml version="1.0" encoding="utf-8" ?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ItemGroup Condition="@(EPiServerApplicationFilesConfigurationTemplate) == ''">
		<EPiServerApplicationFilesConfigurationTemplate Include="$(EPiServerApplicationFilesConfigurationPath)log4net.Template.config">
			<Destination>log4net.Template.config</Destination>
			<Transform>log4net.Common.config</Transform>
		</EPiServerApplicationFilesConfigurationTemplate>
		<EPiServerApplicationFilesConfigurationTemplate Include="$(EPiServerApplicationFilesConfigurationPath)\Web.Template.config">
			<Destination>Web.Template.config</Destination>
			<Transform>Web.Common.config</Transform>
		</EPiServerApplicationFilesConfigurationTemplate>
	</ItemGroup>
	<PropertyGroup>
		<EPiServerVersion Condition="$(EPiServerVersion) == ''">5.2.375.236</EPiServerVersion>
		<EPiServerApplicationFilesConfigurationPath Condition="$(EPiServerApplicationFilesConfigurationPath) == ''">$(SolutionDir)packages\EPiServer-Application-Files.$(EPiServerVersion)\Configuration\</EPiServerApplicationFilesConfigurationPath>
		<EPiServerApplicationFilesMessageImportance Condition="$(EPiServerApplicationFilesMessageImportance) == ''">high</EPiServerApplicationFilesMessageImportance>
	</PropertyGroup>
</Project>